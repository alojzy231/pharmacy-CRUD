// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Products {
  id    Int     @id @default(autoincrement())
  name String  @unique
  substitutes  Products[] @relation("ProductSubstitutes")
  substitutedBy  Products[]  @relation("ProductSubstitutes")
  quantity String
  price Float
  isPrescriptionNeeded Boolean
  category String
  type String
}