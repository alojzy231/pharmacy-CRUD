{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/src/index.js",
    "db:migrate:dev": "prisma migrate dev",
    "dev": "concurrently \"npx tsc -w\" \"tsc-alias -w\" \"nodemon dist/src/index.js\"",
    "generate": "prisma generate",
    "prebuild": "rimraf dist",
    "build": "npx tsc && tsc-alias",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix"
  },
  "devDependencies": {
    "@tsconfig/node16": "1.0.4",
    "@types/express": "4.17.17",
    "@types/node": "17.0.12",
    "concurrently": "8.0.1",
    "nodemon": "2.0.22",
    "rimraf": "5.0.0",
    "tsc-alias": "1.8.6",
    "typescript": "4.5.3"
  },
  "dependencies": {
    "@prisma/client": "4.14.1",
    "express": "4.18.2",
    "prisma": "4.14.1",
    "prisma-zod-generator": "0.8.11",
    "zod": "3.21.4"
  }
}
